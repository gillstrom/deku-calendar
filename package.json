{
  "name": "deku-calendar",
  "version": "1.0.1",
  "description": "Date picker component for Deku",
  "license": "MIT",
  "repository": "gillstrom/deku-calendar",
  "author": {
    "name": "gillstrom",
    "email": "andreasgillstrom@gmail.com",
    "url": "github.com/gillstrom"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "browserify example/index.js -o example/build.js -dv",
    "prepublish": "babel  --presets es2015,react index.js --out-file dist.js",
    "test": "xo"
  },
  "main": "dist.js",
  "files": [
    "dist.js"
  ],
  "keywords": [
    "calendar",
    "component",
    "date",
    "datepicker",
    "deku",
    "dekujs",
    "input",
    "picker",
    "pikaday"
  ],
  "dependencies": {
    "magic-virtual-element": "^1.0.6",
    "object-assign": "^4.0.1",
    "pikaday": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "deku": "^1.0.0",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.9.0",
    "xo": "^0.11.2"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "xo": {
    "esnext": true,
    "extends": [
      "xo-react"
    ],
    "ignores": [
      "dist.js",
      "example/**"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "react/no-unknown-property": 0,
      "quote-props": 0
    }
  }
}
